<?xml version="1.0" ?>

<container
        xmlns="http://symfony.com/schema/dic/services"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/dic/services
    http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="snc_redis.cache.redis" class="Symfony\Component\Cache\Adapter\RedisAdapter" public="false">
            <argument type="service" id="%snc_redis.client%" on-invalid="null" />
        </service>
    </services>

</container>
